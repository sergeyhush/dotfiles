args=( ${(Q)"${(z)$(<<\EO:args
'(--revision)-r+:arg:' '(-r)--revision=:arg:' '(--non-recursive)-N' '(-N)--non-recursive' '--depth=:operation depth (how far to recurse):(empty files immediates infinity)' '--set-depth=[make working copy deeper or shallower]:new depth:(exclude empty files immediates infinity)' '(--quiet)-q' '(-q)--quiet' '--diff3-cmd=:arg:' '--force' '--ignore-externals' '*--cl=:change list:_svn_changelists' '*--changelist=:change list:_svn_changelists' '*--cl=:change list:_svn_changelists' '*--changelist=:change list:_svn_changelists' '--editor-cmd=:arg:' '--accept=:automatic conflict resolution action:((working\:working base\:base recommended\:recommended p\:postpone postpone\:postpone mc\:mine-conflict mine-conflict\:mine-conflict tc\:theirs-conflict theirs-conflict\:theirs-conflict mf\:mine-full mine-full\:mine-full tf\:theirs-full theirs-full\:theirs-full e\:edit edit\:edit l\:launch launch\:launch ))' '--parents' '--adds-as-modification' '--username=:arg:' '--password=:arg:' '--password-from-stdin' '--no-auth-cache' '--non-interactive' '--force-interactive' '--trust-server-cert' '--trust-server-cert-failures=:failures:_values -s , "certificate failures to ignore" "unknown-ca[unknown authority]" "cn-mismatch[hostname mismatch]" "expired[certificate expired]" "not-yet-valid[certificate not yet valid]" "other[all other failures]"' '--config-dir=:config dir:_directories' '*--config-option=: :_svn_config_options'
EO:args
)}"} )
